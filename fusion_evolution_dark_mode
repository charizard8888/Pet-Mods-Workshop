document.getElementById("avgbst").innerHTML=""+avgbst+"";

document.getElementById("modhp").innerHTML=""+modhp+"";
document.getElementById("modatk").innerHTML=""+modatk+"";
document.getElementById("moddef").innerHTML=""+moddef+"";
document.getElementById("modspa").innerHTML=""+modspa+"";
