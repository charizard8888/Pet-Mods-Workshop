document.getElementById("avgspe").innerHTML=""+avgspe+"";
document.getElementById("spe").innerHTML=""+avgspe+"";

let oldwt1 = document.getElementById("oldwt1").value;
let oldwt = document.getElementById("oldwt").value;
let avgwt = (+oldwt1 + +oldwt)/2;
document.getElementById("wt").innerHTML=""+avgwt+"";

let modhp = +hp - +avghp;
let modatk = +atk - +avgatk;
let moddef = +def - +avgdef;
let modspa = +spa - +avgspa;
let modspd = +spd - +avgspd;
let modspe = +spe - +avgspe;
let bst=+hp + +atk + +def + +spa + +spd + +spe;
let modbst =+modhp + +modatk + +moddef + +modspa + +modspd + +modspe
document.getElementById("bst").innerHTML=""+bst+"";
let oldbst =+oldhp + +oldatk + +olddef + +oldspa + +oldspd + +oldspe;
document.getElementById("oldbst").innerHTML=""+oldbst+"";

