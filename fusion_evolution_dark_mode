<script>
function post ()
{
// Front and Part Back End
let fuse1= document.getElementById("fuse1").value;
let fuse2= document.getElementById("fuse2").value;
let fusion = document.getElementById("fusion").value;
let type= document.getElementById("type").value;
let type2= document.getElementById("type2").value;
let hp= document.getElementById("hp").value;
let atk= document.getElementById("atk").value;
let def= document.getElementById("def").value;
let spa= document.getElementById("spa").value;
let spd= document.getElementById("spd").value;
let spe= document.getElementById("spe").value;
let abe= document.getElementById("abe").value;
let abe1= document.getElementById("abe1").value;
let abe2= document.getElementById("abe2").value;
let abedesc= document.getElementById("abedesc").value;
let wt=document.getElementById("wt").value;
let text=document.getElementById("text").value;
// Back End
let mon = fusion.toLowerCase();
let f1 = fuse1.toLowerCase();
let f2 = fuse2.toLowerCase();
f1 = f1.replace(" ","");
f2 = f2.replace(" ","");
let t2=type2;
if (type2==="") {type2 = "],";
}
else {
type2 = ", &quot;"+type2+"&quot;],";
}
let oldhp1 = document.getElementById("oldhp1").value;
let oldhp = document.getElementById("oldhp").value;
let avghp = Math.floor((+oldhp1 + +oldhp)/2);
document.getElementById("avghp").innerHTML=""+avghp+"";
document.getElementById("hp").innerHTML=""+avghp+"";

let oldatk1 = document.getElementById("oldatk1").value;
let oldatk = document.getElementById("oldatk").value;
let avgatk = Math.floor((+oldatk1 + +oldatk)/2);
document.getElementById("avgatk").innerHTML=""+avgatk+"";
document.getElementById("atk").innerHTML=""+avgatk+"";

let olddef1 = document.getElementById("olddef1").value;
let olddef = document.getElementById("olddef").value;
let avgdef = Math.floor((+olddef1 + +olddef)/2);
document.getElementById("avgdef").innerHTML=""+avgdef+"";
document.getElementById("def").innerHTML=""+avgdef+"";

let oldspa1 = document.getElementById("oldspa1").value;
let oldspa = document.getElementById("oldspa").value;
let avgspa = Math.floor((+oldspa1 + +oldspa)/2);
document.getElementById("avgspa").innerHTML=""+avgspa+"";
document.getElementById("spa").innerHTML=""+avgspa+"";

